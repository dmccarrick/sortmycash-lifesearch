{
    "name": "sortmycash/lifesearch",
    "description": "A WordPress plugin to facilitate integration with LifeSearch's API.",
    "type": "wordpress-plugin",
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "SortMyCash\\LifeSearch\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Daniel McCarrick",
            "email": "dmccarrick@hotmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "guzzlehttp/guzzle": "^7.4",
        "ext-simplexml": "*"
    },
    "scripts": {
        "phpcs": "vendor/bin/phpcs --standard=PSR12 --ignore=vendor ."
    }
}
